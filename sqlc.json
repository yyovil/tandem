{
  "version": 2,
  "sql": [
    {
      "engine": "sqlite",
      "schema": "internal/db/migration",
      "queries": "internal/db/sql",
      "gen": {
        "go": {
          "package": "db",
          "out": "internal/db",
          "emit_json_tags": true,
          "emit_empty_slices": true,
          "emit_prepared_queries": true,
          "emit_interface": true,
          "emit_exact_table_names": false
        }
      }
    }
  ]
}